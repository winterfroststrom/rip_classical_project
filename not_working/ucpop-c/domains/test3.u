(define (operator puton)
      :parameters (?X ?Y (block ?Z))
      :precondition (:and (:or (:and (on ?Y ?X) (:and (on ?X ?Z))) (:and (:not (clear ?X)))) 
			  (:not (:or (clear ?Y) (:neq ?Y ?Z) (:not (:not (:eq ?X ?Z)))))
			  (:neq ?X ?Y) (:neq ?X Table))
      :effect (:and (on ?X ?Y) (:not (on ?X ?Z)) 
	        (:neq ?Z Table) (clear ?Z)
       		(:neq ?Y Table) (:not (clear ?Y))))

